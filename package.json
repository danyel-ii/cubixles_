{
  "name": "cubixles_-miniapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "next dev --hostname 127.0.0.1 --port 3000",
    "build": "npm run build:cubixles_scape && next build",
    "postinstall": "node scripts/vercel-postinstall.mjs",
    "start": "next start",
    "dev:cubixles_scape": "npm --workspace apps/cubixles_scape run dev",
    "build:cubixles_scape": "npm --workspace apps/cubixles_scape run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "playwright test",
    "coverage:contracts": "cd contracts && python3 scripts/check_coverage.py",
    "check:no-client-secrets": "npm run build && node scripts/check-client-secrets.mjs",
    "check:no-repo-secrets": "node scripts/check-repo-secrets.mjs",
    "deploy:base": "bash scripts/deploy-base.sh --broadcast",
    "deploy:base:dry": "bash scripts/deploy-base.sh",
    "deploy:mainnet": "bash scripts/deploy-mainnet.sh --broadcast",
    "deploy:mainnet:dry": "bash scripts/deploy-mainnet.sh",
    "deploy:sepolia": "bash scripts/deploy-sepolia.sh --broadcast",
    "deploy:sepolia:dry": "bash scripts/deploy-sepolia.sh",
    "fork-test": "bash scripts/fork-test.sh",
    "bench:api": "node scripts/bench-api.mjs",
    "monitor:swaps": "node scripts/monitor-swaps.mjs"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "9.3.4",
    "@types/react": "19.2.7",
    "jsdom": "24.1.1",
    "next": "^14.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "solhint": "6.0.2",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@farcaster/miniapp-sdk": "^0.2.1",
    "@napi-rs/canvas": "^0.1.62",
    "@upstash/redis": "^1.32.0",
    "@walletconnect/ethereum-provider": "^2.23.1",
    "buffer": "^6.0.3",
    "cannon-es": "^0.20.0",
    "ethers": "^6.16.0",
    "gif-encoder-2": "^1.0.5",
    "p5": "^1.9.2",
    "qrcode": "1.5.3",
    "three": "^0.182.0",
    "zod": "^3.23.8"
  },
  "overrides": {
    "h3": "1.15.5",
    "unstorage": "1.17.4"
  }
}
