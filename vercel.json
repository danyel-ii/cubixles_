{
  "framework": "nextjs",
  "redirects": [
    {
      "source": "/inspecta_deck/",
      "destination": "/inspecta_deck",
      "permanent": true
    },
    {
      "source": "/landing_page/",
      "destination": "/landing_page",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/landing_page",
      "destination": "https://provenance-viewer.vercel.app/inspecta_deck"
    },
    {
      "source": "/landing_page/:path*",
      "destination": "https://provenance-viewer.vercel.app/inspecta_deck/:path*"
    }
  ]
}
