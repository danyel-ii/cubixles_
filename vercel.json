{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "redirects": [
    {
      "source": "/inspecta_deck/",
      "destination": "/inspecta_deck",
      "permanent": true
    },
    {
      "source": "/landing_page",
      "destination": "/inspecta_deck",
      "permanent": false
    },
    {
      "source": "/landing_page/:path*",
      "destination": "/inspecta_deck/:path*",
      "permanent": false
    }
  ]
}
